   "source": [
    "#코사인 유사도 함수\n",
    "def cos_similarity(x,y):\n",
    "    nx=x/np.sqrt(np.sum(x**2)) #x의 정규화\n",
    "    ny=y/np.sqrt(np.sum(y**2)) #y의 정규화\n",
    "    return np.dot(nx,ny)"
   ]
